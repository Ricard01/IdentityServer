        <div class="md:flex md:justify-end w-full sm:w-auto md:h-full py-8 px-4 sm:p-12 md:p-16 md:pt-24 sm:rounded-2xl md:rounded-none sm:shadow md:shadow-none sm:bg-card">
            <div class="w-full max-w-80 sm:w-80 mx-auto sm:mx-0">
                <!-- Logo -->
                <div class="w-20 mx-auto">
                    <img src="~/assets/images/logo.png">

                </div>

                <!-- Title -->
                <div class="mt-8 text-4xl font-extrabold tracking-tight leading-tight">Iniciar Sesion</div>

                <!-- Sign in form -->
                <form asp-page="/Account/Login/Index" class="mt-8">

                    <input type="hidden" asp-for="Input.ReturnUrl" />


                    <partial name="_ValidationSummary" />

                    <!-- Username field -->
                    <div class="w-full mb-4">
                        <label class="block !text-gray-700 text-sm font-bold mb-2" asp-for="Input.Username"></label>
                        <input class=" shadow appearance-none !border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" id="username" placeholder="Username" asp-for="Input.Username" autofocus>
                    </div>




                    <!-- Password field -->
                    <div class="w-full mb-4">
                        <label class="block !text-gray-700 text-sm font-bold mb-2" asp-for="Input.Password"></label>
                        <input class="shadow appearance-none !border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" type="password" placeholder="Password" asp-for="Input.Password" autocomplete="off">
                    </div>


                    <!-- Actions -->
                    @if (Model.View.AllowRememberLogin)
                    {
                        <div class="inline-flex items-end justify-between w-full mt-1.5 mb-4">
                            <div class="inline-flex items-end">
                                <input class="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded" asp-for="Input.RememberLogin">
                                <label class="ml-2 block text-sm text-gray-900" asp-for="Input.RememberLogin">
                                    Remember Me
                                </label>
                            </div>
                        </div>

                    }

                    <!-- Submit button -->

                    <div>
                        <button type="submit" name="Input.Button" value="login" class="group relative w-full flex justify-center py-2 px-4 border border-transparent text-sm font-medium rounded-full text-white bg-darkpurple-400 hover:bg-darkpurple-500 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500">
                            <span class="absolute left-0 inset-y-0 flex items-center pl-3">
                                <!-- Heroicon name: solid/lock-closed -->
                                <svg class="h-5 w-5 text-indigo-500 group-hover:text-indigo-400" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                                    <path fill-rule="evenodd" d="M5 9V7a5 5 0 0110 0v2a2 2 0 012 2v5a2 2 0 01-2 2H5a2 2 0 01-2-2v-5a2 2 0 012-2zm8-2v2H7V7a3 3 0 016 0z" clip-rule="evenodd" />
                                </svg>
                            </span>
                            Ingresar
                        </button>
                    </div>
                </form>


            </div>
        </div>


    }



    @if (!Model.View.EnableLocalLogin && !Model.View.VisibleExternalProviders.Any())
    {
        <div class="alert alert-warning">
            <strong>Invalid login request</strong>
            There are no login schemes configured for this request.
        </div>
    }

